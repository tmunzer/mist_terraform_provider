{
    "datasources": [
        {
            "name": "const_countries",
            "schema": {
                "attributes": [
                    {
                        "name": "const_countries",
                        "set_nested": {
                            "computed_optional_required": "computed",
                            "nested_object": {
                                "attributes": [
                                    {
                                        "name": "alpha2",
                                        "string": {
                                            "computed_optional_required": "computed",
                                            "description": "country code, in two-character"
                                        }
                                    },
                                    {
                                        "name": "certified",
                                        "bool": {
                                            "computed_optional_required": "computed"
                                        }
                                    },
                                    {
                                        "name": "name",
                                        "string": {
                                            "computed_optional_required": "computed"
                                        }
                                    },
                                    {
                                        "name": "numeric",
                                        "number": {
                                            "computed_optional_required": "computed",
                                            "description": "country code, ISO 3166-1 numeric"
                                        }
                                    }
                                ]
                            }
                        }
                    }
                ]
            }
        }
    ],
    "provider": {
        "name": "mistapi"
    },
    "resources": [
        {
            "name": "org_inventory",
            "schema": {
                "attributes": [
                    {
                        "name": "devices",
                        "map_nested": {
                            "validators": [
                                {
                                    "custom": {
                                        "imports": [
                                            {
                                                "path": "github.com/hashicorp/terraform-plugin-framework-validators/mapvalidator"
                                            },
                                            {
                                                "path": "github.com/hashicorp/terraform-plugin-framework-validators/stringvalidator"
                                            },
                                            {
                                                "path": "github.com/Juniper/terraform-provider-mist/internal/validators"
                                            }
                                        ],
                                        "schema_definition": "mapvalidator.KeysAre(\n  stringvalidator.Any(\n    mistvalidator.ParseMagic(),\n    mistvalidator.ParseMac(),\n  ),\n)"
                                    }
                                }
                            ],
                            "computed_optional_required": "computed_optional",
                            "nested_object": {
                                "attributes": [
                                    {
                                        "name": "deviceprofile_id",
                                        "string": {
                                            "computed_optional_required": "computed",
                                            "description": "deviceprofile id if assigned, null if not assigned"
                                        }
                                    },
                                    {
                                        "name": "hostname",
                                        "string": {
                                            "computed_optional_required": "computed",
                                            "description": "hostname reported by the device"
                                        }
                                    },
                                    {
                                        "name": "id",
                                        "string": {
                                            "computed_optional_required": "computed",
                                            "description": "device id"
                                        }
                                    },
                                    {
                                        "name": "mac",
                                        "string": {
                                            "computed_optional_required": "computed",
                                            "description": "device MAC address"
                                        }
                                    },
                                    {
                                        "name": "magic",
                                        "string": {
                                            "computed_optional_required": "computed",
                                            "description": "device claim code"
                                        }
                                    },
                                    {
                                        "name": "model",
                                        "string": {
                                            "computed_optional_required": "computed",
                                            "description": "device model"
                                        }
                                    },
                                    {
                                        "name": "org_id",
                                        "string": {
                                            "computed_optional_required": "computed"
                                        }
                                    },
                                    {
                                        "name": "serial",
                                        "string": {
                                            "computed_optional_required": "computed",
                                            "description": "device serial"
                                        }
                                    },
                                    {
                                        "name": "site_id",
                                        "string": {
                                            "computed_optional_required": "computed_optional",
                                            "description": "Site ID. Used to assign device to a Site"
                                        }
                                    },
                                    {
                                        "name": "type",
                                        "string": {
                                            "computed_optional_required": "computed",
                                            "description": "enum: `ap`, `gateway`, `switch`"
                                        }
                                    },
                                    {
                                        "name": "unclaim_when_destroyed",
                                        "bool": {
                                            "computed_optional_required": "computed_optional",
                                            "default": {
                                                "static": false
                                            },
                                            "description": "Unclaim the device from the Mist Organization when removed from the provider inventory"
                                        }
                                    },
                                    {
                                        "name": "vc_mac",
                                        "string": {
                                            "computed_optional_required": "computed",
                                            "description": "only if `type`==`switch`, MAC Address of the Virtual Chassis"
                                        }
                                    }
                                ]
                            },
                            "description": "Can be the device Claim Code or the device MAC Address:\n  * Claim Code: used to claim the device to the Mist Organization and manage it. Format is `[0-9A-Z]{15}` (e.g `01234ABCDE56789`)\n  * MAC Address: used to managed a device already in the Mist Organization (claimed or adopted devices). Format is `[0-9a-f]{12}` (e.g `5684dae9ac8b`)\nRemoving a device from the list will NOT release it unless `unclaim_when_destroyed` is set to `true`"
                        }
                    },
                    {
                        "name": "org_id",
                        "string": {
                            "computed_optional_required": "required"
                        }
                    }
                ]
            }
        }
    ],
    "version": "0.1"
}