												"name": "bounce",
												"string": {
													"computed_optional_required": "computed_optional"
												}
											},
											{
												"name": "engaged",
												"string": {
													"computed_optional_required": "computed_optional"
												}
											},
											{
												"name": "passerby",
												"string": {
													"computed_optional_required": "computed_optional"
